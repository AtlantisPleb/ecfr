{
  "name": "ecfr",
  "repository": "https://github.com/AtlantisPleb/ecfr.git",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "prisma generate && next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:migrate": "prisma migrate deploy",
    "db:clean": "NODE_OPTIONS=\"--loader ts-node/esm\" ts-node scripts/clean.ts",
    "ingest": "ts-node-esm scripts/ingest/run.ts",
    "ingest:clean": "pnpm run db:clean && pnpm run ingest",
    "studio": "prisma studio",
    "t": "tsc --noEmit"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.4.1",
    "@types/ms": "^0.7.32",
    "@types/node": "20.8.3",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.11",
    "autoprefixer": "10.4.16",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.4",
    "ms": "^2.1.3",
    "next": "13.5.4",
    "postcss": "8.4.31",
    "prisma": "^5.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  }
}